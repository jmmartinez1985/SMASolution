@model IEnumerable<SMAWeb.Models.SolicitudViewModel>

@{
    ViewBag.Title = "Index";
}

<link href="@Url.Content("~/Content/jpages/jPages.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/jpages/animate.css")" rel="stylesheet" type="text/css" />
@*<link href="@Url.Content("~/Content/jpages/style.css")" rel="stylesheet" type="text/css" />*@
<script src="@Url.Content("~/Scripts/jpage/jPages.min.js")" type="text/javascript"></script>


<br />
<div class="container">
    <div class="row-fluid margin-bottom-10">

        <div class="another-page">
            <div class="headline">
                <h3>Formulario Solicitud de Servicio</h3>
            </div>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>

                            @Html.DisplayNameFor(model => model.Solicitud)
                        </th>
                        <th>

                            @Html.DisplayNameFor(model => model.FechaCreacion)
                        </th>
                        <th>

                            @Html.DisplayNameFor(model => model.Solicitante)
                        </th>
                        <th>

                            @Html.DisplayNameFor(model => model.EmailSolicitante)
                        </th>
                        <th>

                            @Html.DisplayNameFor(model => model.TelefonoSolicitante)
                        </th>
                        <th>

                            @Html.DisplayNameFor(model => model.Status)
                        </th>
                     
                    </tr>
                </thead>
                <tbody id="divBody">


                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>
                               @* @Html.DisplayFor(modelItem => item.Solicitud)*@

                                @Html.ActionLink(item.Solicitud.ToString(), "Details", "SolicitudServicio", new { id = item.Solicitud }, null)

                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FechaCreacion)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Solicitante)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EmailSolicitante)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TelefonoSolicitante)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Status)
                            </td>
         
                        </tr>
                    }
                </tbody>

                <tfoot>
                    <tr>
                        <td colspan="7">
                            <div class="holder" style="float: left">
                            </div>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div>
<script>
    var $jpage = jQuery.noConflict();
    $jpage(function () {
        $jpage("div.holder").jPages({
            containerID: "divBody",
            //              previous: "?",
            //              next: "?",
            first: "Primera",
            previous: "Anterior",
            next: "Siguiente",
            last: "Última",
            perPage: 5,
            delay: 20
        });
    });
</script>
