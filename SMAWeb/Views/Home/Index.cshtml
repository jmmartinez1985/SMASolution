<script src="~/Scripts/SMA.js"></script>
<script src="~/Scripts/knockout-2.2.0.js"></script>

<script type="text/javascript">
    jQuery(function ()
    {
        Index.initParallaxSlider();
        HOME.GetServices('@Url.Action("GetServices", "Home")', '@Url.Action("Details", "Anuncios")');
        HOME.SearchKey();
        HOME.ChangeCategory("CAT_Id", '@Url.Action("GetSubCategories", "SubCategoriaServicio")');
        $(".SearchButton").click(function ()
        {
            var attr = $(this).attr('data-val');
            HOME.CallFilter(attr, '@Url.Action("GetInformationAnuncios", "Anuncios")', '@Url.Action("Details", "Anuncios")');
        });
        HOME.VerifyCredential('@Url.Action("VerifyCompleteCredential", "Home")', '@Url.Action("EditUser", "UserProfile")');
        HOME.VerifySolicitudesPendientes('@Url.Action("VerifySolicitudesPendientes", "Home")', '@Url.Action("SolicitudRequest", "SolicitudServicio")');
    });

    function CheckVisibleProjects()
    {
        var rows = jQuery('#anunciosAvailable > tr');
        if (rows.filter(':hidden').size() == rows.length)
        {
            jQuery('#divNoAnunciosFound').fadeIn(1000);
        }
        else
        {
            jQuery('#divNoAnunciosFound').hide();
        }
    }

    function SeeAnuncios(anuncioid)
    {
        var url = '@Url.Action("Details", "Anuncios")' + "/" + anuncioid;
        window.location = url;
    }

    function TakeService(anuncioid)
    {
        var titulo = $("#btnTakeService" + anuncioid).attr("data-titulo");
        var nombre = $("#btnTakeService" + anuncioid).attr("data-nombre");
        HOME.TakeService(anuncioid, '@Url.Action("TakeService", "SolicitudServicio")', nombre, titulo);
    }

    function ReportarSpam(anuncioid)
    {
        var titulo = $("#btnTakeService" + anuncioid).attr("data-titulo");
        var nombre = $("#btnTakeService" + anuncioid).attr("data-nombre");
        HOME.ReportarSpam(anuncioid, '@Url.Action("Spam", "Anuncios")', nombre, titulo);
    }



</script>
