    <table class="table-hover" style="width: 100%">
        <thead>
        </thead>
        <tbody id="anunciosAvailable">
        </tbody>
        <tfoot>
            <tr>
                <td colspan="1">
                    <div class="holder" style="float: left">
                    </div>
                </td>
            </tr>
        </tfoot>

    </table>
</div>

<div class="span9" id="SecondSearch">
    <table class="table-hover" style="width: 100%">
        <thead>
        </thead>
        <tbody id="SearchData">
        </tbody>
        <tfoot>
            <tr>
                <td colspan="1">
                    <div class="holder" style="float: left">
                    </div>
                </td>
            </tr>
        </tfoot>

    </table>
</div>


