@{
    ViewBag.Title = "Home Page";
}



<!--=== Purchase Block ===-->
<div class="row-fluid purchase margin-bottom-30">
    <div class="container">
        <div class="span9">
            <span>Service Market servicios persona a persona.</span>
            <p>
                Aquí usted podrá buscar los servicios que necesite sin tener que hacer filas, llamadas innecesarias y sobre todo desde cualquier parte del mundo y en el horario que le sea más conveniente.
                <a href="@Url.Content("~/Home/About")" style="color:#e67e22">Conozca más</a>
            </p>

        </div>
        <a href="@Url.Content("~/Anuncios/Create")" class="btn-u btn-buy hover-effect">¡Publica Ya!</a>
    </div>
</div>
<!--/row-fluid-->
<!-- End Purchase Block -->

@*@if(Request.IsAuthenticated && ViewBag.IsIncomplete)
{
<div class="row-fluid">
    <div class="alert alert-error">
         <strong><a href="@Url.Action("UserProfile","EditUser")">Haga click aquí</a></strong> para completar el registro de su perfil.
        
      </div>
    <br />
</div>
}*@
<div class="slider-inner">
    <div id="da-slider" class="da-slider">
        <div class="da-slide">
            <h2><i>Service Market</i>
                <br />
                <i>En todos tus</i>
                <br />
                <i>Dispositivos</i></h2>
            <p>
                <i>Ingresa a Service Market desde</i>
                <br />
                <i>tu computadora, celular</i>
                <br />
                <i>o tablet</i>
            </p>
            <div class="da-img">
                <img src="@Url.Content("~/Images/4.png")" alt="" />
            </div>
        </div>
        <div class="da-slide">
            <h2><i>Encuentra los mejores</i>
                <br />
                <i>servicios y publica</i>
                <br />
                <i>los tuyos grátis</i></h2>
            <p>
                <i>Todo en un solo sitio</i>
                <br />
                <i>y en el horario que elijas</i>
            </p>
            <div class="da-img" style="top: 12px;">
                <img src="@Url.Content("~/Images/2.png")" alt="" style="width: 48%;" />
            </div>
        </div>
        <div class="da-slide">
            <h2><i>Mantente siempre en</i>
                <br />
                <i>contacto con </i>
                <br />
                <i>Service Market</i></h2>
            <p>
                <i>Síguenos en</i>
                <br />
                <i>Facebook y </i>
                <br />
                <i>Twitter </i>
            </p>
            <div class="da-img">
                <img src="@Url.Content("~/Images/3.png")" alt="image01" />
            </div>
        </div>
        <nav class="da-arrows">
            <span class="da-arrows-prev"></span>
            <span class="da-arrows-next"></span>
        </nav>
    </div>
    <!--/da-slider-->
</div>



<br />


<div class="span9">


    <div class="row-fluid purchase">
        <span>Búsqueda de Servicios</span>
        <div class="form-inline">
            <div class="input-append">
                <input id="txtSearchHome" class="input-xxlarge" type="text" placeholder="Buscar en la página">
                <button type="submit" class="btn-u btn-u-orange"><i class="icon-search icon-white"></i></button>
            </div>
            <a href="#advanced" role="button" data-toggle="modal" class="btn-u btn-u-orange btn-large">Búsqueda Avanzada</a>
        </div>

    </div>
    <br />
</div>
